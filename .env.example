# Groq API Key (Required for LiteLLM)
GROQ_API_KEY=your_groq_api_key_here

# Model Configuration (Groq models via LiteLLM)
# Options: llama-3.3-70b-versatile, llama-3.1-8b-instant, mixtral-8x7b-32768
MODEL_NAME=groq/llama-3.3-70b-versatile

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=true

# Agent Configuration
AGENT_NAME=MortgageAdvisor
DEFAULT_INTEREST_RATE=4.5
MAX_LTV_EXPAT=0.80
TRANSFER_FEE_PERCENT=4.0
AGENCY_FEE_PERCENT=2.0
MISC_FEES_PERCENT=1.0
MAX_TENURE_YEARS=25
